---
import Layout from '../layouts/Layout.astro';
import Hero from '../components/Pages/Home/Hero';
import '../styles/global.scss';
import DefaultHero from '../components/Layout/DefaultHero';
import ProjectGallery from '../components/Pages/Projects/ProjectGallery';
import CallToAction from '../components/Layout/CallToAction';


const heroData = {
  title: "Our Projects",
  description: "Explore some of our recent renovation projects, from stunning deck constructions to complete kitchen and bathroom remodels. Each project reflects our commitment to quality and attention to detail.",
};
---

<Layout>
  <DefaultHero 
    title={heroData.title} 
    description={heroData.description} />
  <ProjectGallery />
  <CallToAction />
</Layout>
